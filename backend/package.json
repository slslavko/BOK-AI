{
  "name": "bok-ai-backend",
  "version": "1.0.0",
  "description": "BOK-AI Secure Multi-Tenant Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "docker:build": "docker build -t bok-ai-backend .",
    "docker:run": "docker run -p 3001:3001 bok-ai-backend"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/websocket": "^10.0.1",
    "@qdrant/js-client-rest": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.4.1",
    "minio": "^8.0.1",
    "openai": "^4.67.3",
    "pdf-parse": "^1.1.1",
    "pg": "^8.12.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "postgres": "^3.4.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.8",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.17.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "chatbot",
    "e-commerce",
    "multi-tenant",
    "fastify",
    "typescript"
  ],
  "author": "BOK-AI Team",
  "license": "UNLICENSED"
}
